{
  "name": "speed-mern-app-demo-backend",
  "version": "1.0.0",
  "description": "Speed your MERN app by REDIS",
  "main": "src/server.ts",
  "scripts": {
    "es-linting": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "lint": "tsc --noEmit && npm run es-linting",
    "build": "npx tsc && npm run lint",
    "start-main": "concurrently -k \"npx tsc --watch\" \"nodemon  dist/src/server.js\"",
    "data": "node dist/data/main.js",
    "write-behind": "node data/write-behind/wb-main.js",
    "write-through": "node data/write-through/wt-main.js",
    "start": "npm run build && npm run start-main",
    "docker-start": "npm run build && npm run data && npm run write-behind && npm run start-main",
    "docker-wt-start": "npm run build && npm run data && npm run write-through && npm run start-main"
  },
  "keywords": [
    "Redis",
    "MERN"
  ],
  "author": "PrasanKumar",
  "dependencies": {
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "mongo-seeding": "^3.7.2",
    "mongodb": "^4.8.0",
    "redis": "^4.3.0",
    "redis-om": "^0.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}